<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm} %-5level [%thread] %logger{36} - %msg%n</pattern>
<!--            <pattern>%d{HH:mm} %-5level %logger{12} - %msg%n </pattern>-->
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>